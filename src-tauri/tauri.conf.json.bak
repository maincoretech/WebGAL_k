{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:3000",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "untitled",
    "version": "0.0.2"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "AdventureGame",
      "copyright": "copyright maincore_tech.2024",
      "android": {
        "minSdkVersion": 24
      },
      "appimage": {
        "bundleMediaFramework": true
      },
      "deb": {
        "files": {}
      },
      "iOS": {},
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.maincore.untitled",
      "longDescription": "A way to touch untitled world",
      "macOS": {
        "minimumSystemVersion": "10.13"
      },
      "targets": [
        "app",
        "appimage",
        "nsis",
        "dmg"
      ],
      "updater": {
        "active": false,
        "pubkey": "",
        "windows": {
          "installMode": "passive"
        }
      },
      "windows": {
        "allowDowngrades": true,
        "certificateThumbprint": null,
        "digestAlgorithm": null,
        "nsis": {
          "compression": "lzma",
          "installerIcon": "icons/installer-icon/icon.ico"
        },
        "timestampUrl": null,
        "tsp": false,
        "webviewFixedRuntimePath": null,
        "webviewInstallMode": {
          "silent": true,
          "type": "downloadBootstrapper"
        },
        "wix": null
      }
    },
    "macOSPrivateApi": true,
    "pattern": {
      "use": "brownfield"
    },
    "security": {
      "assetProtocol": {
        "enable": false,
        "scope": []
      },
      "dangerousDisableAssetCspModification": false,
      "dangerousRemoteDomainIpcAccess": [],
      "freezePrototype": false
    },
    "windows": [
      {
        "height": 1440,
        "width": 2560,
        "resizable": true,
        "title": "UNtitled",
        "minHeight": 480,
        "minWidth": 720
      }
    ]
  }
}